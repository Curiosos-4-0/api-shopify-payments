generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Purshase {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  gpo_id    String   @unique
  amount    String
  pos       String
  reference String
  status    String?
}

model Reference {
  id          Int      @id @default(autoincrement())
  reference   String   @unique
  order_id    String
  amount      String
  endDateTime String
  status      String
  createdAt   DateTime @default(now())
}